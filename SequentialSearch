public class SequentialSearch{public static void main(String[] args){ int[] One = {2, 9, 6, 7, 4, 5, 3, 0, 1};int target = 4; sequentialSearch(One,target);}public static void sequentialSearch(int[] a, int b){ int index = -1;for (int i = 0; i <a.length; i++) { if (a[i]== b) {index= i; break;}}if (index== -1) {System.out.println("Your target integer does not exist in the array");} else {System.out.println("Your target integer is in index " + "index+of the array");}}}